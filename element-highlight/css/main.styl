#content, aside
  -webkit-box-sizing border-box
  position absolute
  top 0
  left 0
  height 100%
  width 100%
#content
  background #EEE
  padding 0 0 0 300px
  z-index 1
  iframe
    width 100%
    height 100%
aside
  width 300px
  padding 10px
  z-index 3
  overflow scroll
  input
    width 100%